@model Tachyon.Models.Procedure

@{
    ViewData["Title"] = "ProcedureCreate";
    Layout = "~/Views/Shared/_MainLayout-Special.cshtml";
}
}

<div>

<h4>Procedure</h4>
<hr />
<div class="row">
    <div class="col-sm">
        <form class="row g-3" asp-action="ProcedureCreate">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-md-2">
                    <label class="control-label">name</label>
                <input asp-for="name" class="form-control" />
                <span asp-validation-for="name" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                    <label class="control-label">Last Name</label>
                <input asp-for="lastname" class="form-control" />
                <span asp-validation-for="lastname" class="text-danger"></span>
            </div>
            <div class="col-md-2"> 
                <label asp-for="dob" class="control-label">Date Of Birth</label>
                <input asp-for="dob" class="form-control" />
                <span asp-validation-for="dob" class="text-danger"></span>
            </div>
            <div class="col-md-3">
                    <label class="control-label">Age</label>
                <input asp-for="Age" class="form-control" />
                <span asp-validation-for="Age" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                    <label class="control-label">Gender</label>
                <input asp-for="Gender" class="form-control" />
                <span asp-validation-for="Gender" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                    <label  class="control-label">Phone</label>
                <input asp-for="phone" class="form-control" />
                <span asp-validation-for="phone" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                    <label  class="control-label">Date</label>
                <input asp-for="Date" class="form-control" />
                <span asp-validation-for="Date" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label class="control-label">Start Time</label>
                <input asp-for="StartTime" class="form-control" />
                <span asp-validation-for="StartTime" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label class="control-label">End Time</label>
                <input asp-for="EndTime" class="form-control" />
                <span asp-validation-for="EndTime" class="text-danger"></span>
            </div>
            <div class="col-md-2">
                <label  class="control-label">Room</label>
                <input asp-for="Room" class="form-control" />
                <span asp-validation-for="Room" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                <label class="control-label">Anesthesia</label>
                <input asp-for="Anesthesia" class="form-control" />
                <span asp-validation-for="Anesthesia" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Anesthesia Type</label>
                <input asp-for="anesthesiaType" class="form-control" />
                <span asp-validation-for="anesthesiaType" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Physician</label>
                <input asp-for="physician" class="form-control" />
                <span asp-validation-for="physician" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Anesthesiologist</label>
                <input asp-for="Anesthesiologist" class="form-control" />
                <span asp-validation-for="Anesthesiologist" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Assistant</label>
                <input asp-for="Assistant" class="form-control" />
                <span asp-validation-for="Assistant" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Instrument Operator</label>
                <input asp-for="InstrumentOperator" class="form-control" />
                <span asp-validation-for="InstrumentOperator" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Description</label>
                <input asp-for="description" class="form-control" />
                <span asp-validation-for="description" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Complications</label>
                <input asp-for="complications" class="form-control" />
                <span asp-validation-for="complications" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Post Procedure</label>
                <input asp-for="postProcedure" class="form-control" />
                <span asp-validation-for="postProcedure" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Instructions</label>
                <input asp-for="instructions" class="form-control" />
                <span asp-validation-for="instructions" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">Disposition</label>
                <input asp-for="disposition" class="form-control" />
                <span asp-validation-for="disposition" class="text-danger"></span>
            </div>
            <div class="col-md-5">
                    <label class="control-label">notes</label>
                <input asp-for="notes" class="form-control" />
                <span asp-validation-for="notes" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a type="button" class="btn btn-primary" asp-action="ProcedureIndex">Back to List</a>
</div>

</div>
<link href="~/specialised/style - copy.css" rel="stylesheet" />
<link href="~/specialised/style.css" rel="stylesheet" />
